   <div class="row">
      <div class="col-md-12 ">
        <h3 class="black"><%= @title%> Orders</h3>
           <% unless @orders.blank? %>
                    <span>
                    <ul class=" list-unstyled pull-right" style="margin-top: -29px; width: 200px; margin-right: 10px;">
                    <li class="dropdown">
                        <a href="#" class="dropdown-toggle menu-item" data-toggle="dropdown" aria-expanded="false"> More</a>
                        <ul class="dropdown-menu well">
                            <li><a href="#"> Option 1</a></li>
                            <li><a href="#"> Option 2</a></li>
                            <li><a href="#"> Option 3</a></li>
                            <li><a href="#"> Option 4</a></li>
                        </ul>
                    </li>
                    </ul>
                    </span>
              <%end%>      

         <% if @orders.present?%>
            <%= pie_chart(@orders) %>
         <%else%>
         <div style="background: none repeat scroll 0% 0% snow; height: 100px;"> 
          <p class="text-center text-danger pad10">
            <%= "No Orders for today"%>
          </p>  
          </div>  
         <%end%>         

      </div>
    </div>  
      
      <div class="row">
      <div class="col-md-12">
           <h3>Visits By Day</h3>
          <%= bar_chart(current_account.orders.today.map { |m| [m.first_name, m.id] })%>
     
      </div>
   </div>   


   <div class="row">
      <div class="col-md-12 ">
          <h3>Most Selled Category</h3>
          <%= line_chart @products.group('date(created_at), name').count, discrete: true %>

      </div>
   </div>

   <div class="row">
      <div class="col-md-12">
          <h3>Todays Locations of users</h3>
         <%= geo_chart ({"India" => 10, "China" => 5, "USA" => 25, "Canada" => 50}) %>
      </div>

   </div>
   <div class="row">
      <div class="col-md-12">
          <h3>Non Interested Products</h3>
         <%= column_chart  (@products.map { |m| [m.name, m.id] }) %>

      </div>

   </div>

